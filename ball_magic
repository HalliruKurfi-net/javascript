fill(0, 0, 0);
ellipse(200, 200, 375, 375);
fill(60, 0, 255);
triangle(200, 104, 280, 280, 120, 280);
fill(255, 255, 255);
var answer = floor(random(1, 5));
if (answer === 3) {
    text("PLAY", 176, 200);
    text("THE GAME", 159, 229);
}
else if(answer === 4){
    text("Continue", 176, 200);
}else if(answer > 1){
    text("Finish", 176, 200);

}
